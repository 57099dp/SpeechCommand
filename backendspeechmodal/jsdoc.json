{
  "source": {
    "include": ["index.js"],
    "includePattern": ".js$",
    "exclude": ["node_modules"]
  },
  "opts": {
    "destination": "./docs",
    "recurse": true,
    "template": "node_modules/docdash"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true,
      "staticFiles": {
        "include": []
      }
    },
    "docdash": {
      "static": true,
      "sort": true,
      "search": true,
      "collapse": true,
      "wrap": true,
      "typedefs": true,
      "navLinks": [
        {
          "label": "Readme",
          "href": "readme.html"
        },
        {
          "label": "GitHub",
          "href": "https://github.com/your/project"
        }
      ]
    }
  }
}
